{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
	"env": {
		browser: true,
        es2020: true
	},
    "rules": {
        "prettier/prettier": "error",
        "no-console": 0,
        "no-alert": 0
    }
}